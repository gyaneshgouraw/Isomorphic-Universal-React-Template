  "main": "start.js",
  "scripts": {
    "start": "NODE_PATH=\"./src\" node --harmony ./start",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client.js",
    "test": "node_modules/.bin/mocha --compilers js:babel-core/register test-setup.js 'src/**/*.test.js'",
    "watch-client": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client-watch.js && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.client-watch.js",
    "watch": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\" \"npm run start\""
  },
  "dependencies": {
    "babel": "6.1.18",
    "babel-core": "6.1.19",
    "babel-polyfill": "6.1.19",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "babel-preset-stage-0": "6.1.18",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "history": "1.13.0",
    "koa": "1.1.2",
    "koa-proxy": "0.4.1",
    "koa-static": "1.5.2",
    "node-sass": "^3.4.2",
    "piping": "0.3.0",
    "react": "0.14.2",
    "react-dom": "0.14.2",
    "react-redux": "^4.0.5",
    "react-router": "^1.0.0",
    "redux": "^3.0.5",
    "redux-simple-router": "^1.0.2",
    "redux-thunk": "^1.0.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0"
  },
  "devDependencies": {
    "babel-loader": "6.1.0",
    "chai": "^3.4.1",
    "concurrently": "0.1.1",
    "jsdom": "^7.2.1",
    "json-loader": "0.5.3",
    "mocha": "^2.3.4",
    "pre-push": "^0.1.1",
    "react-addons-test-utils": "^0.14.3",
    "react-hot-loader": "1.3.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "webpack": "1.12.4",
    "webpack-dev-server": "1.12.1"
  },
  "engines": {
    "node": ">=0.10.32"
  },
  "pre-push": [
    "npm test"
  ]
